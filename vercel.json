{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "backend/requirements.txt",
          "backend/models/**",
          "backend/data/**",
          "backend/config/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
